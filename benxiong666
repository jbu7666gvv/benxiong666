            Variant = "Secondary",
        },
        {
            Title = "‰ΩøÁî®",
            Icon = "arrow-right",
            Callback = function() 
                Confirmed = true 
                WindUI:Notify({Title = "Ê¨¢Ëøé", Content = "ÂºÄÂßã‰ΩøÁî®Êú¨ÁÜäÊ±âÂåñ", Duration = 2})
            end,
            Variant = "Primary",
        }
    }
