-- å…¨å±€å˜é‡
local translationCache = {}
local originalTexts = {}
local isTranslating = false
local rescanConnection = nil
local lastRescanTime = 0

-- ç®€åŒ–çš„ç¿»è¯‘å‡½æ•°
local function translateText(text)
    if type(text) ~= "string" or text == "" then
        return text
    end
    
    -- æ£€æŸ¥ç¼“å­˜
    if translationCache[text] then
        return translationCache[text]
    end
    
    local translated = text
    
    -- ç›´æ¥éå†è¯å…¸è¿›è¡Œæ›¿æ¢
    for eng, chn in pairs(translationDict) do
        if string.find(text, eng) then
            translated = string.gsub(translated, eng, chn)
        end
    end
    
    translationCache[text] = translated
    return translated
end

-- ä¸€æ¬¡æ€§å¤„ç†æ‰€æœ‰GUIå¯¹è±¡
local function processAllGUIObjects()
    if not isTranslating then return 0 end
    
    local objectsProcessed = 0
    
    -- å¤„ç†PlayerGuiä¸­çš„æ‰€æœ‰å¯¹è±¡
    local function processContainer(container)
        local descendants = container:GetDescendants()
        for _, obj in ipairs(descendants) do
            if not isTranslating then break end
            
            if obj:IsA("TextLabel") or obj:IsA("TextButton") or obj:IsA("TextBox") then
                local success, result = pcall(function()
                    local textToTranslate = nil
                    local propertyToTranslate = nil
                    
                    if obj:IsA("TextLabel") or obj:IsA("TextButton") then
                        if obj.Text and obj.Text ~= "" then
                            textToTranslate = obj.Text
                            propertyToTranslate = "Text"
                        end
                    elseif obj:IsA("TextBox") then
                        if obj.PlaceholderText and obj.PlaceholderText ~= "" then
                            textToTranslate = obj.PlaceholderText
                            propertyToTranslate = "PlaceholderText"
                        elseif obj.Text and obj.Text ~= "" then
                            textToTranslate = obj.Text
                            propertyToTranslate = "Text"
                        end
                    end
                    
                    if textToTranslate and propertyToTranslate then
                        local translated = translateText(textToTranslate)
                        if translated ~= textToTranslate then
                            -- å­˜å‚¨åŸå§‹æ–‡æœ¬
                            if not originalTexts[obj] then
                                originalTexts[obj] = {}
                            end
                            originalTexts[obj][propertyToTranslate] = textToTranslate
                            
                            -- åº”ç”¨ç¿»è¯‘
                            obj[propertyToTranslate] = translated
                            objectsProcessed = objectsProcessed + 1
                        end
                    end
                end)
            end
        end
    end
    
    -- å¤„ç†æ‰€æœ‰GUIå®¹å™¨
    processContainer(playerGui)
    processContainer(CoreGui)
    
    return objectsProcessed
end

-- æ¢å¤åŸå§‹æ–‡æœ¬
local function restoreOriginalTexts()
    local restoredCount = 0
    for obj, properties in pairs(originalTexts) do
        if obj and obj.Parent then
            for property, value in pairs(properties) do
                pcall(function()
                    obj[property] = value
                    restoredCount = restoredCount + 1
                end)
            end
        end
    end
    
    originalTexts = {}
    translationCache = {}
    return restoredCount
end

-- å¼€å§‹å®šæœŸé‡æ–°æ‰«æ
local function startPeriodicRescan()
    if rescanConnection then
        rescanConnection:Disconnect()
        rescanConnection = nil
    end
    
    rescanConnection = RunService.Heartbeat:Connect(function()
        if isTranslating then
            local currentTime = tick()
            if currentTime - lastRescanTime >= 30 then
                lastRescanTime = currentTime
                local count = processAllGUIObjects()
                print("[è‡ªåŠ¨æ±‰åŒ–] 30ç§’é‡æ–°æ±‰åŒ–å®Œæˆï¼Œå¤„ç†äº† " .. count .. " ä¸ªæ–‡æœ¬")
            end
        end
    end)
end

-- åœæ­¢é‡æ–°æ‰«æ
local function stopPeriodicRescan()
    if rescanConnection then
        rescanConnection:Disconnect()
        rescanConnection = nil
    end
end

-- åˆ›å»ºæ§åˆ¶UI
local function createControlUI()
    -- ç­‰å¾…PlayerGuiå®Œå…¨åŠ è½½
    while not playerGui do
        wait(0.1)
        playerGui = player:FindFirstChild("PlayerGui")
    end
    
    -- æ£€æŸ¥æ˜¯å¦å·²ç»å­˜åœ¨UI
    local existingUI = playerGui:FindFirstChild("AutoTranslatorUI")
    if existingUI then
        existingUI:Destroy()
    end
    
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "AutoTranslatorUI"
    screenGui.ResetOnSpawn = false
    screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    screenGui.Parent = playerGui
    
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 300, 0, 150)
    mainFrame.Position = UDim2.new(0, 20, 0, 20)
    mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    mainFrame.BorderSizePixel = 0
    mainFrame.Parent = screenGui
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 12)
    corner.Parent = mainFrame
    
    local stroke = Instance.new("UIStroke")
    stroke.Color = Color3.fromRGB(100, 100, 150)
    stroke.Thickness = 2
    stroke.Parent = mainFrame
    
    -- æ ‡é¢˜æ 
    local titleBar = Instance.new("Frame")
    titleBar.Size = UDim2.new(1, 0, 0, 35)
    titleBar.Position = UDim2.new(0, 0, 0, 0)
    titleBar.BackgroundColor3 = Color3.fromRGB(45, 45, 60)
    titleBar.BorderSizePixel = 0
    titleBar.Parent = mainFrame
    
    local titleCorner = Instance.new("UICorner")
    titleCorner.CornerRadius = UDim.new(0, 12)
    titleCorner.Parent = titleBar
    
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, -10, 1, 0)
    title.Position = UDim2.new(0, 10, 0, 0)
    title.BackgroundTransparency = 1
    title.Text = "ğŸŒ è‡ªåŠ¨æ±‰åŒ–å·¥å…· v2.0"
    title.TextColor3 = Color3.fromRGB(255, 255, 255)
    title.TextSize = 16
    title.Font = Enum.Font.GothamBold
    title.TextXAlignment = Enum.TextXAlignment.Left
    title.Parent = titleBar
    
    -- å†…å®¹åŒºåŸŸ
    local contentFrame = Instance.new("Frame")
    contentFrame.Size = UDim2.new(1, 0, 1, -35)
    contentFrame.Position = UDim2.new(0, 0, 0, 35)
    contentFrame.BackgroundTransparency = 1
    contentFrame.Parent = mainFrame
    
    -- åˆ‡æ¢æŒ‰é’®
    local toggleButton = Instance.new("TextButton")
    toggleButton.Size = UDim2.new(0.9, 0, 0, 45)
    toggleButton.Position = UDim2.new(0.05, 0, 0.1, 0)
    toggleButton.BackgroundColor3 = Color3.fromRGB(65, 105, 225)
    toggleButton.Text = "ğŸš€ å¼€å¯æ±‰åŒ–"
    toggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    toggleButton.TextSize = 16
    toggleButton.Font = Enum.Font.GothamBold
    toggleButton.Parent = contentFrame
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 8)
    buttonCorner.Parent = toggleButton
    
    local buttonStroke = Instance.new("UIStroke")
    buttonStroke.Color = Color3.fromRGB(255, 255, 255)
    buttonStroke.Thickness = 1.5
    buttonStroke.Parent = toggleButton
    
    -- çŠ¶æ€æ ‡ç­¾
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Size = UDim2.new(0.9, 0, 0, 20)
    statusLabel.Position = UDim2.new(0.05, 0, 0.6, 0)
    statusLabel.BackgroundTransparency = 1
    statusLabel.Text = "ğŸ“Š çŠ¶æ€: å·²å…³é—­"
    statusLabel.TextColor3 = Color3.fromRGB(200, 200, 255)
    statusLabel.TextSize = 14
    statusLabel.Font = Enum.Font.Gotham
    statusLabel.Parent = contentFrame
    
    local infoLabel = Instance.new("TextLabel")
    infoLabel.Size = UDim2.new(0.9, 0, 0, 15)
    infoLabel.Position = UDim2.new(0.05, 0, 0.8, 0)
    infoLabel.BackgroundTransparency = 1
    infoLabel.Text = "â° æ¯30ç§’è‡ªåŠ¨é‡æ–°æ±‰åŒ–"
    infoLabel.TextColor3 = Color3.fromRGB(150, 150, 180)
    infoLabel.TextSize = 12
    infoLabel.Font = Enum.Font.Gotham
    infoLabel.Parent = contentFrame
    
    -- æŒ‰é’®ç‚¹å‡»äº‹ä»¶
    toggleButton.MouseButton1Click:Connect(function()
        isTranslating = not isTranslating
        
        if isTranslating then
            -- å¼€å¯æ±‰åŒ–
            toggleButton.Text = "ğŸ›‘ å…³é—­æ±‰åŒ–"
            toggleButton.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
            statusLabel.Text = "ğŸ“Š çŠ¶æ€: æ±‰åŒ–ä¸­..."
            
            -- ç«‹å³æ‰§è¡Œä¸€æ¬¡å®Œæ•´æ±‰åŒ–
            spawn(function()
                local count = processAllGUIObjects()
                statusLabel.Text = "ğŸ“Š çŠ¶æ€: æ±‰åŒ–ä¸­ | å·²å¤„ç† " .. count .. " ä¸ªæ–‡æœ¬"
                print("[è‡ªåŠ¨æ±‰åŒ–] é¦–æ¬¡æ±‰åŒ–å®Œæˆï¼Œå¤„ç†äº† " .. count .. " ä¸ªæ–‡æœ¬")
                
                -- å¯åŠ¨30ç§’å®šæœŸé‡æ–°æ‰«æ
                startPeriodicRescan()
            end)
        else
            -- å…³é—­æ±‰åŒ–
            toggleButton.Text = "ğŸš€ å¼€å¯æ±‰åŒ–"
            toggleButton.BackgroundColor3 = Color3.fromRGB(65, 105, 225)
            statusLabel.Text = "ğŸ“Š çŠ¶æ€: å·²å…³é—­"
            
            -- åœæ­¢é‡æ–°æ‰«æå¹¶æ¢å¤æ–‡æœ¬
            stopPeriodicRescan()
            local restoredCount = restoreOriginalTexts()
            print("[è‡ªåŠ¨æ±‰åŒ–] å·²å…³é—­æ±‰åŒ–ï¼Œæ¢å¤äº† " .. restoredCount .. " ä¸ªæ–‡æœ¬")
        end
    end)
    
    -- é¼ æ ‡æ‚¬åœæ•ˆæœ
    toggleButton.MouseEnter:Connect(function()
        if isTranslating then
            toggleButton.BackgroundColor3 = Color3.fromRGB(200, 50, 80)
        else
            toggleButton.BackgroundColor3 = Color3.fromRGB(75, 115, 245)
        end
    end)
    
    toggleButton.MouseLeave:Connect(function()
        if isTranslating then
            toggleButton.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
        else
            toggleButton.BackgroundColor3 = Color3.fromRGB(65, 105, 225)
        end
    end)
    
    print("[è‡ªåŠ¨æ±‰åŒ–] æ§åˆ¶UIåˆ›å»ºæˆåŠŸï¼")
    return screenGui
end

-- ä¸»åˆå§‹åŒ–å‡½æ•°
local function initialize()
    -- ç­‰å¾…æ¸¸æˆå®Œå…¨åŠ è½½
    wait(2)
    
    -- åˆ›å»ºæ§åˆ¶UI
    local success, errorMsg = pcall(createControlUI)
    if not success then
        warn("[è‡ªåŠ¨æ±‰åŒ–] åˆ›å»ºUIå¤±è´¥: " .. tostring(errorMsg))
        return
    end
    
    print("====================================")
    print("[è‡ªåŠ¨æ±‰åŒ–] åŠ è½½æˆåŠŸï¼ç‰ˆæœ¬ 2.0")
    print("[è‡ªåŠ¨æ±‰åŒ–] æ¨¡å¼: ä¸€æ¬¡æ€§å…¨éƒ¨æ±‰åŒ– + 30ç§’é‡å¤")
    print("[è‡ªåŠ¨æ±‰åŒ–] ç‚¹å‡»å·¦ä¸Šè§’æŒ‰é’®å¼€å§‹æ±‰åŒ–")
    print("====================================")
end

-- å¯åŠ¨è„šæœ¬
spawn(initialize)
