    })

    Tabs.General:Section({Title = "通用功能"})

    Tabs.General:Toggle({
        Title = "夜视",
        Desc = "开启/关闭夜视",
        Default = false,
        Locked = false,
        Callback = function(value)
            if value then
                game.Lighting.Ambient = Color3.new(1, 1, 1)
                game.Lighting.Brightness = 1
            else
                game.Lighting.Ambient = Color3.new(0, 0, 0)
                game.Lighting.Brightness = 1
            end
        end
    })

    Tabs.General:Button({
