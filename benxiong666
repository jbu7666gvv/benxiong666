local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

function gradient(text, startColor, endColor)
    local result = ""
    local length = #text
    
    for i = 1, length do
        local t = (i - 1) / math.max(length - 1, 1)
        local r = math.floor((startColor.R + (endColor.R - startColor.R) * t) * 255)
        local g = math.floor((startColor.G + (endColor.G - startColor.G) * t) * 255)
        local b = math.floor((startColor.B + (endColor.B - startColor.B) * t) * 255)

        local char = text:sub(i, i)
        result = result .. "<font color=\"rgb(" .. r ..", " .. g .. ", " .. b .. ")\">" .. char .. "</font>"
    end
    
    return result
end

local function loadScript(url, scriptName)
    WindUI:Notify({Title = "ÊèêÁ§∫", Content = "Ê≠£Âú®Âä†ËΩΩ " .. scriptName .. "...", Duration = 2})
    
    local success, result = pcall(function()
        local response = game:HttpGet(url, true)
        if not response or #response < 50 then
            return false, "ËÑöÊú¨ÂÜÖÂÆπÊó†ÊïàÊàñ‰∏∫Á©∫"
        end
        
        local loadSuccess, loadError = pcall(loadstring, response)
        if not loadSuccess then
            return false, "ËÑöÊú¨ÊâßË°åÈîôËØØ: " .. tostring(loadError)
        end
        
        return true, "Âä†ËΩΩÊàêÂäü"
    end)
    
    if success and result == true then
        WindUI:Notify({Title = "ÊàêÂäü", Content = scriptName .. " Âä†ËΩΩÂÆåÊàê", Duration = 3})
    else
        local errorMsg = success and result or "ÁΩëÁªúËØ∑Ê±ÇÂ§±Ë¥•"
        WindUI:Notify({Title = "ÈîôËØØ", Content = scriptName .. " Âä†ËΩΩÂ§±Ë¥•: " .. errorMsg, Duration = 5})
    end
end

local function setupChineseLocalization()
    if not game then
        WindUI:Notify({Title = "ÈîôËØØ", Content = "Ê∏∏ÊàèÁéØÂ¢ÉÊú™ÊâæÂà∞", Duration = 3})
        return false
    end
    
    if not getrawmetatable then
        WindUI:Notify({Title = "Ë≠¶Âëä", Content = "ÂΩìÂâçÊâßË°åÂô®‰∏çÊîØÊåÅÂÖÉË°®Êìç‰Ωú", Duration = 3})
        return false
    end
    
    local success, m = pcall(getrawmetatable, game)
    if not success or not m then
        WindUI:Notify({Title = "ÈîôËØØ", Content = "Êó†Ê≥ïËé∑ÂèñÊ∏∏ÊàèÂÖÉË°®", Duration = 3})
        return false
    end
    
    local originalNewIndex = m.__newindex
    
    local translationTable = {
        ["TOOLS"] = "Â∑•ÂÖ∑",
        ["INF JUMP"] = "Êó†ÈôêË∑≥Ë∑É",
        ["DESVNC BODY"] = "Ë∫´‰Ωì‰∏çÂêåÊ≠•",
        ["3RD FLOOR"] = "‰∏âÊ•º",
        ["FLY TO BASE"] = "È£ûÂæÄÂü∫Âú∞",
        ["TELEPORT"] = "‰º†ÈÄÅ",
        ["SPEED"] = "ÈÄüÂ∫¶",
        ["FLY"] = "È£ûË°å",
        ["NOCLIP"] = "Á©øÂ¢ô",
        ["GOD MODE"] = "‰∏äÂ∏ùÊ®°Âºè"
    }

    local setReadonlySuccess = pcall(setreadonly, m, false)
    if not setReadonlySuccess then
        WindUI:Notify({Title = "ÈîôËØØ", Content = "Êó†Ê≥ï‰øÆÊîπÂÖÉË°®ÊùÉÈôê", Duration = 3})
        return false
    end
    
    if typeof(newcclosure) == "function" then
        m.__newindex = newcclosure(function(t, k, v)
            if k == "Text" and (t:IsA("TextLabel") or t:IsA("TextButton") or t:IsA("TextBox")) then
                v = translationTable[v] or v
            end
            return originalNewIndex(t, k, v)
        end)
    else
        m.__newindex = function(t, k, v)
            if k == "Text" and (t:IsA("TextLabel") or t:IsA("TextButton") or t:IsA("TextBox")) then
                v = translationTable[v] or v
            end
            return originalNewIndex(t, k, v)
        end
    end
    
    pcall(setreadonly, m, true)
    WindUI:Notify({Title = "ÊàêÂäü", Content = "Ê±âÂåñÁéØÂ¢ÉËÆæÁΩÆÂÆåÊàê", Duration = 2})
    return true
end

