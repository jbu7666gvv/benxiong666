local function globalErrorHandler(err)
    warn("æœ¬ç†Šè„šæœ¬é”™è¯¯: " .. tostring(err))
    if WindUI then
        WindUI:Notify({
            Title = "ç³»ç»Ÿé”™è¯¯",
            Content = "å‘ç”Ÿé”™è¯¯: " .. tostring(err),
            Duration = 10,
            Color = Color3.fromRGB(255, 0, 0)
        })
    end
end

